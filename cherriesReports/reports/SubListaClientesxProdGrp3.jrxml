	<parameter name="dsProductora" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
-- Empresa cliente
	   cl.t_negocio tipo,
       p.nombre pais,
	   cd.nombre cdad,
       cl.nombre cliente,
       cl.direc direc
FROM 
     fah_contratos c,
     fah_empresascliente cl,
     fah_paises p,
     fah_ciudades cd
WHERE 
      c.id_prod = $P{dsProductora} AND c.estatus = 'Activo' AND
      c.id_client = cl.id AND
      cl.id_pais = p.id AND
      p.id = cd.id_pais AND
      cl.id_cdad = cd.id
ORDER BY	cl.t_negocio,
			p.nombre,
			cd.nombre]]>
	</queryString>
